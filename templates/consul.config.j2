{
     "advertise_addr": "{{ ansible_eth1.ipv4.address }}",
     "bind_addr": "{{ ansible_eth1.ipv4.address }}",
     "bootstrap_expect": 3,
     "client_addr": "0.0.0.0",
     "datacenter": "DC1",
     "data_dir": "/var/lib/consul",
     "domain": "consul",
     "enable_script_checks": true,
     "dns_config": {
         "enable_truncate": true,
         "only_passing": true
     },
     "enable_syslog": true,
     "encrypt": "{{ consul_cluster_key }}",
     "leave_on_terminate": true,
     "log_level": "INFO",
     "rejoin_after_leave": true,
     "retry_join": [
         "{{ nodes_consul_1 }}",
         "{{ nodes_consul_2 }}",
         "{{ nodes_consul_3 }}"
     ],
     "server": true,
     "start_join": [
         "{{ nodes_consul_1 }}",
         "{{ nodes_consul_2 }}",
         "{{ nodes_consul_1 }}"
     ],
     "ui": true
}